{
  "name": "SqaAgent",
  "productName": "SQA-Agent",
  "version": "3.0.0",
  "email": "rohan.kar@simplify3x.com",
  "description": "SimplifyQA",
  "main": "main2.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --publish always"
  },
  "repository": "https://s3.ap-south-1.amazonaws.com/agent.simplifyqa.app/mac",
  "publish": {
    "provider": "s3",
    "bucket": "agent.simplifyqa.app",
    "path": "mac/",
    "region": "ap-south-1"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "build": {
    "appId": "SimplifyQA",
    "mac": {
      "target": [
        "pkg",
        "zip"
      ],
      "icon": "simplifylogo.ico",
      "extraResources": [
        "./libs/**",
        "com.simplifyQA.Agent.jar",
        "./com.simplifyQA.Agent_lib/**",
        "./codeEditorProjects/**",
        "./JRE_1.8/**"
      ]
    },
    "afterPack": "./scripts/afterPackage.js"
  },
  "author": "SimplifyQA",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^21.3.1",
    "electron-builder": "^23.6.0"
  },
  "dependencies": {
    "@electron/remote": "^2.0.9",
    "aws-sdk": "^2.1348.0",
    "cross-fetch": "^3.1.5",
    "electron-log": "^5.0.0-beta.16",
    "electron-packager": "^17.1.1",
    "electron-root-path": "^1.1.0",
    "electron-updater": "^5.3.0",
    "express": "^4.18.2",
    "node-fetch": "^1.7.3",
    "request": "^2.88.2",
    "socket.io": "^4.5.4",
    "tree-kill": "^1.2.2",
    "update-electron-app": "^2.0.1",
    "url-exist": "^3.0.1",
    "xhr2": "^0.2.1"
  }
}
