{
  "name":"SqaAgent",
  "productName": "SQA Agent",
  "version": "3.0.0",
  "email": "rohan.kar@simplify3x.com",
  "description": "SimplifyQA",
  "main": "main2.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder "
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "build": {
    "appId": "SimplifyQA",
    "win": {
      "icon": "./build/simplifylogo.ico",
      "publish": null,
      "target": [
        "nsis"
      ],
      "extraFiles": [
        {
          "from": "com.simplifyQA.Agent.jar",
          "to": ".",
          "filter": [
            "**/*"
          ]
        },
        {
          "from": "./libs",
          "to": "./libs",
          "filter": [
            "**/*"
          ]
        },
        {
          "from": "./com.simplifyQA.Agent_lib",
          "to": "./com.simplifyQA.Agent_lib",
          "filter": [
            "**/*"
          ]
        },
        {
          "from": "./codeEditorProjects",
          "to": "./codeEditorProjects",
          "filter": [
            "**/*"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "linux": {
      "maintainer": "simplifyqa",
      "target": [
        "deb"
      ],
      "icon": "./libs/images/loader_1.png",
      "extraFiles": [
        {
          "from": "com.simplifyQA.Agent.jar",
          "to": ".",
          "filter": [
            "**/*"
          ]
        }
      ],
      "category": "Graphics"
    },
    "afterPack": "./scripts/afterPackage.js",
    "files": []
  },
  "author": "SimplifyQA",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^21.3.1",
    "electron-builder": "^23.6.0"
  },
  "dependencies": {
    "@electron/remote": "^2.0.9",
    "cross-fetch": "^3.1.5",
    "electron-packager": "^17.1.1",
    "electron-root-path": "^1.1.0",
    "electron-updater": "^5.3.0",
    "express": "^4.18.2",
    "fs-extra": "^11.1.0",
    "node-fetch": "^1.7.3",
    "request": "^2.88.2",
    "socket.io": "^4.5.4",
    "tree-kill": "^1.2.2",
    "update-electron-app": "^2.0.1",
    "url-exist": "^3.0.1",
    "xhr2": "^0.2.1"
  }
}